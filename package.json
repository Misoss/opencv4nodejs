{
  "name": "opencv4nodejs-prebuilt",
  "version": "4.15.1-beta1",
  "description": "This is a fork of opencv4nodejs which provides pre-built binaries",
  "keywords": [
    "opencv",
    "cv",
    "computer vision",
    "face",
    "detection",
    "recognition",
    "machine",
    "learning",
    "neural",
    "network",
    "async",
    "typescript"
  ],
  "author": {
    "name": "Simon Hofmann",
    "email": "dev@simon-hofmann.org",
    "url": "https://simon-hofmann.org"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nut-tree/opencv4nodejs"
  },
  "bugs": {
    "url": "https://github.com/nut-tree/opencv4nodejs/issues"
  },
  "homepage": "https://github.com/nut-tree/opencv4nodejs#readme",
  "main": "./lib/opencv4nodejs.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "deps": "node ./install.js",
    "install": "prebuild-install --verbose || npm run rebuild",
    "configure": "node-gyp configure",
    "build": "npm run deps && node-gyp configure build",
    "rebuild": "npm run deps && node-gyp rebuild",
    "clean": "node-gyp clean",
    "prebuild": "npm run deps && prebuild",
    "build-debug": "npm run deps && node-gyp rebuild --debug"
  },
  "gypfile": true,
  "dependencies": {
    "@nut-tree/opencv-build-darwin": "^3.4.6-patch",
    "macro-inferno": "^0.2.3",
    "nan": "^2.12.1",
    "native-node-utils": "^0.1.5",
    "prebuild-install": "^5.3.0"
  },
  "optionalDependencies": {
    "@types/node": ">6"
  },
  "devDependencies": {
    "prebuild": "^8.2.0"
  },
  "opencv": "3.4.6-patch"
}
